<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="item"
            type="com.huishan.enjoytravel.data.remote.entity.MovingStatisticsEntity" />
        <variable
            name="viewmodel"
            type="com.huishan.enjoytravel.ui.operation.MovingStatisticsViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_common_white_radius_bg">

        <TextView
            android:id="@+id/tv_label"
            android:layout_width="90dp"
            android:layout_height="25dp"
            android:layout_marginEnd="30dp"
            android:background="@drawable/shape_card_type_receive"
            android:gravity="center"
            android:text="成功"
            android:textColor="@color/white"
            android:textSize="12sp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_move_car_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/default_horizontal_offset"
            android:layout_marginTop="@dimen/default_vertical_offset"
            android:text="挪车类型："
            android:textColor="@color/default_black_color"
            android:textSize="14sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_move_car_type_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ssssss"
            android:textColor="@color/default_black_color"
            android:textSize="14sp"
            app:layout_constraintLeft_toRightOf="@id/tv_move_car_type"
            app:layout_constraintTop_toTopOf="@id/tv_move_car_type" />

        <TextView
            android:id="@+id/tv_scan_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="扫描时间："
            android:textColor="@color/default_black_color"
            android:textSize="14sp"
            app:layout_constraintLeft_toLeftOf="@id/tv_move_car_type"
            app:layout_constraintTop_toBottomOf="@id/tv_move_car_type" />

        <TextView
            android:id="@+id/tv_scan_time_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ssssss"
            android:textColor="@color/default_black_color"
            android:textSize="14sp"
            app:layout_constraintLeft_toRightOf="@id/tv_scan_time"
            app:layout_constraintTop_toTopOf="@id/tv_scan_time" />

        <TextView
            android:id="@+id/tv_move_car_consume"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="挪车耗时："
            android:textColor="@color/default_black_color"
            android:textSize="14sp"
            app:layout_constraintLeft_toLeftOf="@id/tv_move_car_type"
            app:layout_constraintTop_toBottomOf="@id/tv_scan_time" />

        <TextView
            android:id="@+id/tv_move_car_consume_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ssssss"
            android:textColor="@color/default_black_color"
            android:textSize="14sp"
            app:layout_constraintLeft_toRightOf="@id/tv_move_car_consume"
            app:layout_constraintTop_toTopOf="@id/tv_move_car_consume" />


        <TextView
            android:id="@+id/tv_bike_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="@dimen/default_vertical_offset"
            android:text="车辆编号："
            android:textColor="@color/default_black_color"
            android:textSize="14sp"
            app:layout_constraintLeft_toLeftOf="@id/tv_move_car_type"
            app:layout_constraintTop_toBottomOf="@id/tv_move_car_consume" />

        <TextView
            android:id="@+id/tv_bike_number_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ssssss"
            android:textColor="@color/default_black_color"
            android:textSize="14sp"
            app:layout_constraintLeft_toRightOf="@id/tv_bike_number"
            app:layout_constraintTop_toTopOf="@id/tv_bike_number" />

        <Button
            android:id="@+id/bt_browse"
            android:layout_width="160dp"
            android:layout_height="32dp"
            android:layout_marginTop="@dimen/default_vertical_offset"
            android:layout_marginBottom="@dimen/default_vertical_offset"
            android:background="@drawable/shape_rectangle_blue_content_radius_10dp"
            android:text="查看挪车轨迹"
            android:textColor="@color/white"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@id/tv_bike_number"
            app:layout_constraintTop_toBottomOf="@id/tv_bike_number_value" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>